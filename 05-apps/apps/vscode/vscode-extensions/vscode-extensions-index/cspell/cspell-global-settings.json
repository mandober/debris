{
    "version": "0.2",
    "name":                 "my_global_config",
    "description":          "My global config for cspell",
    "allowCompoundWords":   true,
    "caseSensitive":        false,
    "maxDuplicateProblems": 5,
    "maxNumberOfProblems":  20,
