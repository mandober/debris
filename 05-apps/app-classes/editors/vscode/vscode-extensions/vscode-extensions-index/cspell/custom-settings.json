{
    "version": "0.2",
    // "name":                 "my_custom_config",
    // "description":          "My custom config for custom cspell dict",

    // ENABLE THIS CONF
    "enabled":              true,

    // CUSTOM DICTIONARIES
    "customDictionaries": {
        // Disable the internal-terms dictionary
        "internal-terms": true,
        // Enable custom dictionaries
        "custom": true,
        // define my custom dictionary
        "my-custom-dictionary": {
            "name": "my-custom-dictionary",
            "description": "My custom dictionary",
            // path may use predefined vars like ${workspaceFolder:debriefs}
            "path": "T:/code/projects/debris/.vscode/dict_debrief.json",
            // If true words will be added to this dictionary
            "addWords": true
        }
    }
}
